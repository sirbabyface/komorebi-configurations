{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.37/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.json",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "default_workspace_padding": 0,
  "default_container_padding": 3,
  "border": true,
  "border_width": 5,
  "border_offset": -1,
  "border_style": "System",
  "mouse_follows_focus": false,
  "border_colours": {
    "single": "#f3ae35",
    "stack": "#4b95e9",
    "monocle": "#ec1021",
    "unfocused": "#2d2d2d"
  },
  "animation": {
    "enabled": false,
    "duration": 250,
    "fps": 60,
    "style": "EaseOutSine"
  },
  "scrolling-layout-columns": 1,
  "ignore_rules": [
    [
      {
        "kind": "Exe",
        "id": "Flow.Launcher.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "matching_strategy": "Equals",
        "id": "Action keyword Setting"
      }
    ],
    [
      {
        "kind": "Exe",
        "id": "TopNotify.exe",
        "matching_strategy": "Equals"
      }
    ],
    [
      {
        "kind": "Exe",
        "id": "Flow.Launcher.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "matching_strategy": "Equals",
        "id": "Restart Computer"
      }
    ],
    [
      {
        "kind": "Exe",
        "id": "fman.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "matching_strategy": "StartsWith",
        "id": "Packing"
      }
    ],
    [
      {
        "kind": "Title",
        "matching_strategy": "StartsWith",
        "id": "Bitwarden"
      }
    ],
    [
      {
        "kind": "Exe",
        "matching_strategy": "Equals",
        "id": "VC_redist.x64.exe"
      }
    ],
    [
      {
        "kind": "Exe",
        "id": "yasb.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "matching_strategy": "StartsWith",
        "id": "Failed"
      }
    ],
    [
      {
        "kind": "Exe",
        "id": "slack.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "matching_strategy": "Equals",
        "id": "Neuraspace"
      }
    ]
  ],
  "manage_rules": [
    [
      {
        "kind": "Exe",
        "id": "paintdotnet.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "matching_strategy": "Contains",
        "id": "Paint.NET"
      }
    ]
  ],
  "floating_applications": [
    [
      {
        "kind": "Title",
        "id": "ShareX - ",
        "matching_strategy": "StartsWith"
      },
      {
        "kind": "Exe",
        "id": "ShareX.exe",
        "matching_strategy": "Equals"
      }
    ],
    [
      {
        "kind": "Title",
        "matching_strategy": "Equals",
        "id": "Phone Link"
      },
      {
        "kind": "Exe",
        "matching_strategy": "Equals",
        "id": "PhoneExperienceHost.exe"
      }
    ],
    [
      {
        "kind": "Exe",
        "matching_strategy": "Equals",
        "id": "WindowsTerminal.exe"
      }
    ],
    [
      {
        "kind": "Exe",
        "matching_strategy": "Equals",
        "id": "Taskmgr.exe"
      }
    ],
    [
      {
        "kind": "Title",
        "matching_strategy": "Equals",
        "id": "Window Spy for AHKv2"
      }
    ],
    [
      {
        "kind": "Title",
        "matching_strategy": "StartsWith",
        "id": "Sign in - Google Accounts"
      }
    ]
  ],
  "remove_titlebar_applications":[
    [
      {
        "kind": "Exe",
        "matching_strategy": "Equals",
        "id": "fman.exe"
      }
    ]
  ],
  "display_index_preferences": {
    "0": "402NTJJC0684",
    "1": "0"
  },
  "monitors": [
    {
      "window_based_work_area_offset_limit": 1,
      "window_based_work_area_offset": {
        "left": 400,
        "right": 800,
        "bottom": 0,
        "top": 0
      },
      "workspaces": [
        {
          "name": "Main",
          "layout": "UltrawideVerticalStack"
        }
      ]
    },
    {
      "workspaces": [
        {
          "name": "Main",
          "layout": "VerticalStack"
        }
      ]
    }
  ]
}
